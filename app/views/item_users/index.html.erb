<h1>Корзина</h1>
<table class = "table">
  <tr>
    <td>Название товара</td>
    <td>Цена</td>
    <td>Количество</td>
    <td></td>

  </tr>
  <% @item_users.each do |t| %>
  <tr>
    <td><%= t.item.name %></td>
    <td><%= t.item.price %></td>
    <td>1</td>
    <td><%= link_to 'Удалить',t, method: :delete, confirm: 'Точно?' %> </td>
  </tr>
<% end %>

</table>